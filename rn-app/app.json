{
  "expo": {
    "name": "rn-app",
    "slug": "rn-app",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.weareroot.plantechta",
      "permissions": [
        "INTERNET"
      ],
      "versionCode": 1
    },
    "ios": {
      "bundleIdentifier": "com.weareroot.plantechta",
      "supportsTablet": true
    }
  }
}
